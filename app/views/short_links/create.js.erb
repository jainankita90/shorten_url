<% if @short_link.errors.messages.present? == false%>
$('#result').html('<p class="short_url">Your Link <%= @short_link.main_url%>: <%= link_to "#{ENV['HOST_URL']}/short_links/fetch/#{@short_link.slug}", controller:'short_links', action:'fetch', slug:@short_link.slug %></p>')
<% else %>
$('#result').html('<p class="short_url" style="color:red">Some error occured, Please verify URL again.  </p>')
<% end %>
